    "build-storybook": "lerna run build-storybook",
    "clean:node_modules": "lerna clean && rimraf node_modules",
    "cypress:admin-ui:open": "cypress open --config integrationFolder=cypress/admin-ui,supportFile=cypress/admin-ui/support/index.js",
    "cypress:admin-ui:run": "cypress run --config integrationFolder=cypress/admin-ui,supportFile=cypress/admin-ui/support/index.js",
    "cypress:clean": "rimraf cypress/screenshots/* cypress/videos/*",
    "cypress:consumer-ui:open": "cypress open --config integrationFolder=cypress/consumer-ui,supportFile=cypress/consumer-ui/support/index.js",
    "cypress:consumer-ui:run": "cypress run --config integrationFolder=cypress/consumer-ui,supportFile=cypress/consumer-ui/support/index.js",
    "generate": "lerna run generate",
    "lint": "lerna run lint -- --fix",
    "start:dev": "lerna run start:dev --stream",
    "storybook": "lerna run storybook --stream",
    "storybook:ci": "lerna run storybook:ci --stream",
    "test:unit": "lerna run test:unit",
    "test:unit:ci": "lerna run test:unit:ci",
    "test:unit:updateSnapshot": "lerna run test:unit:updateSnapshot"
  },
