{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.detectIndentation": false,
  "editor.fontSize": 14,
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "[python]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "[js]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  }
}
